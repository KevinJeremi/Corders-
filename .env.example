# =================================
# CORDERS - Environment Variables
# =================================
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to git!

# =================================
# Kolosal AI Configuration
# =================================
# Get your API key from https://kolosal.ai
KOLOSAL_API_KEY=kol_your_api_key_here

# =================================
# Google Cloud Vision API
# =================================
# Option 1: Paste the entire JSON content of your service account
# (Recommended for Vercel/cloud deployments)
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"...","client_email":"..."}

# Option 2: Path to your service account JSON file
# (For local development only - file must NOT be in git)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account.json
